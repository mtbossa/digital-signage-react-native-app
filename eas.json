    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      }
    },
    "development-prod": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      }
    },
    "staging": {
